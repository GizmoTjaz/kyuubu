{
  "name": "kyuubu",
  "version": "1.0.0",
  "description": "3D cube renderer written in vanilla JS using the HTML canvas. All classes include an \"acceptable\" amount of JSDoc documentation.",
  "main": "src/kyuubu.js",
  "directories": {
    "example": "examples"
  },
  "scripts": {
    "ts-build": "rm -rf ./build && tsc",
    "ts-build-watch": "tsc -w",
    "es-build": "esbuild ./build/index.js --bundle --minify --outfile=dist/kyuubu.bundle.js",
    "build": "npm run ts-build && npm run es-build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GizmoTjaz/kyuubu.git"
  },
  "author": "GizmoTjaz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/GizmoTjaz/kyuubu/issues"
  },
  "homepage": "https://github.com/GizmoTjaz/kyuubu#readme",
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/plugin-proposal-class-properties": "^7.12.13",
    "@babel/preset-env": "^7.12.16",
    "babel-loader": "^8.2.2",
    "typescript": "^4.1.5"
  },
  "dependencies": {
    "esbuild": "^0.10.2"
  }
}
